{
    "name": "vanvu/laravel-menu",
    "description": "drag and drop menu generator like wordpress for laravel 5, support multi language",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "authors": [
    	{
            "name": "Dinh Van Vu",
            "email": "dinhvanvu94@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Pham Manh Thang",
            "email": "phammanhthang@gmail.com",
            "role" : "Original Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "5.8.* || ^6.0"
    },
    "autoload": {
        "psr-4": {
          "Harimayco\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harimayco\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Harimayco\\Menu\\Facades\\Menu"
            }
        }
    }
}
