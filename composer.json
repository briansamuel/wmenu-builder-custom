{
    "name": "manhthang/laravel-menu",
    "description": "drag and drop menu generator like wordpress for laravel 5",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder", "Multiple Language"],
    "authors": [
    	{
            "name": "Manh Thang",
            "email": "phammanhthang95@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "5.8.* || ^6.0"
    },
    "autoload": {
        "psr-4": {
          "Harimayco\\Menu\\": "packages/manhthang/laravel-menu/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harimayco\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Harimayco\\Menu\\Facades\\Menu"
            }
        }
    }
}
